{
  "name": "redux-cycles",
  "version": "0.4.1",
  "description": "Bring functional reactive programming to Redux using Cycle.js",
  "typings": "index.d.ts",
  "main": "dist",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "repository": "https://github.com/cyclejs-community/redux-cycles",
  "contributors": [
    {
      "name": "Luca Matteis"
    },
    {
      "name": "Nick Balestra"
    },
    {
      "name": "Gosha Arinich"
    }
  ],
  "scripts": {
    "eslint": "eslint --fix src/ test/",
    "test": "npm run build && npm run eslint && jest",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/example"
    ]
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "dependencies": {
    "@cycle/run": "*"
  },
  "peerDependencies": {
    "xstream": "*"
  },
  "devDependencies": {
    "@cycle/rxjs-run": "^4.1.0",
    "rxjs": "^5.2.0",
    "babel-cli": "^6.18.0",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.15.0",
    "jest": "^18.1.0",
    "redux": "^3.6.0"
  }
}
